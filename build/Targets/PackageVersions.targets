<Project>

    <PropertyGroup>
        <AvaloniaVersion>0.8.1-cibuild0002143-beta</AvaloniaVersion>
        <AvaloniaSkiaLinuxNativesVersion>1.68.0.2</AvaloniaSkiaLinuxNativesVersion>
        <MicrosoftBuildVersion>16.0.461</MicrosoftBuildVersion>
        <NuGetVersion>5.0.0</NuGetVersion>
        <ReactiveUIVersion>9.13.1</ReactiveUIVersion>
        <RoslynAnalyzersVersion>3.0.0-beta2.19218.6</RoslynAnalyzersVersion>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Update="Avalonia" Version="$(AvaloniaVersion)" />
        <PackageReference Update="Avalonia.Desktop" Version="$(AvaloniaVersion)" />
        <PackageReference Update="Avalonia.ReactiveUI" Version="$(AvaloniaVersion)" />
        <PackageReference Update="Avalonia.Skia.Linux.Natives" Version="$(AvaloniaSkiaLinuxNativesVersion)" />
        <PackageReference Update="Microsoft.Build" Version="$(MicrosoftBuildVersion)" />
        <PackageReference Update="Microsoft.Build.Utilities.Core" Version="$(MicrosoftBuildVersion)" />
        <PackageReference Update="NuGet.Frameworks" Version="$(NuGetVersion)" />
        <PackageReference Update="NuGet.Versioning" Version="$(NuGetVersion)" />
        <PackageReference Update="ReactiveUI" Version="$(ReactiveUIVersion)" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Update="@(RoslynAnalyzer)" Version="$(RoslynAnalyzersVersion)" />
    </ItemGroup>

</Project>
